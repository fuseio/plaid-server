{
  "name": "node-skeleton",
  "version": "0.1.0",
  "description": "node-skeleton",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --trace-warnings ./src/app.js",
    "debug": "node --inspect-brk --trace-warnings ./src/app.js",
    "start-prod": "node ./src/app.js",
    "doc": "apidoc -i src -o docs/api && apidoc-markdown -p docs/api -o docs/api.md",
    "lint": "standard",
    "lint-fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonprou/node-skeleton.git"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "globals": [
      "assert"
    ],
    "ignore": [
      "docs"
    ]
  },
  "_moduleAliases": {
    "@constants": "./src/constants",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@utils": "./src/utils",
    "@services": "./src/services"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "async": "^3.2.0",
    "aws-sdk": "^2.799.0",
    "body-parser": "^1.19.0",
    "config": "^3.3.2",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-jwt": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "methods": "^1.1.2",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "mongoose": "^5.10.15",
    "morgan": "^1.10.0",
    "plaid": "^8.1.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@rigwild/apidoc-markdown": "^5.0.0",
    "apidoc": "^0.25.0",
    "nodemon": "^2.0.6",
    "standard": "^16.0.3"
  }
}
